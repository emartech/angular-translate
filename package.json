{
  "name": "@emartech/angular-translate",
  "description": "Translate module for Angular",
  "keywords": [
    "angular",
    "translate",
    "i18n"
  ],
  "main": "dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "test": "mocha --require reflect-metadata --require ts-node/register lib/**/*.spec.ts",
    "prepare": "npm run build",
    "semantic-release": "semantic-release",
    "build": "rimraf dist && ngc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emartech/angular-translate.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emartech/angular-translate/issues"
  },
  "release": {
    "verifyConditions": {
      "path": "./node_modules/semantic-release/src/lib/plugin-noop.js"
    }
  },
  "homepage": "https://github.com/emartech/angular-translate#readme",
  "engines": {
    "node": "10.16.3"
  },
  "dependencies": {
    "@angular/cli": "8.3.17",
    "flat": "5.0.0",
    "ramda": "0.26.1",
    "sprintf-js": "1.1.2"
  },
  "peerDependencies": {
    "@angular/core": "^8.0.0"
  },
  "devDependencies": {
    "@angular/compiler": "8.2.13",
    "@angular/compiler-cli": "8.2.13",
    "@angular/core": "8.2.13",
    "@types/chai": "4.2.4",
    "@types/flat": "0.0.28",
    "@types/mocha": "5.2.7",
    "@types/node": "12.12.6",
    "@types/ramda": "0.26.33",
    "@types/sprintf-js": "1.1.2",
    "chai": "4.2.0",
    "mocha": "6.2.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.0",
    "rxjs": "6.5.3",
    "semantic-release": "15.13.30",
    "ts-node": "8.4.1",
    "typescript": "3.5.3",
    "typings": "2.1.1",
    "zone.js": "0.9.1"
  }
}
